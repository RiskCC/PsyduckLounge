{
	"version": "1.0.0-*",
	"description": "DiscordBot",
	"authors": [ "Risk" ],
	"tags": [ "discord", "discordapp" ],
	"projectUrl": "https://github.com/riskcc/UmiBot",
	"licenseUrl": "http://opensource.org/licenses/MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/riskcc/UmiBot"
  },
  "exclude": [ "config" ],
  "compilationOptions": {
    "emitEntryPoint": true
  },

	"commands": {
		"start": "DiscordBot"
	},

	"configurations": {
		"PrivateDebug": {
			"compilationOptions": {
				"define": [
					"DEBUG",
					"TRACE",
					"PRIVATE"
				]
			}
		},
		"PrivateRelease": {
			"compilationOptions": {
				"define": [
					"RELEASE",
					"PRIVATE"
				]
			}
		}
	},

  "dependencies": {
    "Discord.Net": "0.9.0-rc3-3",
    "Discord.Net.Audio": "0.9.0-rc3",
    "Discord.Net.Commands": "0.9.0-rc3-1",
    "Discord.Net.Modules": "0.9.0-rc3",
    "Newtonsoft.Json": "8.0.2"
  },

  "frameworks": {
    "dnx451": {
      "frameworkAssemblies": {
        "System.Net": "4.0.0.0",
        "System.Net.Http": "4.0.0.0",
        "System.Xml": "4.0.0.0",
        "System.Xml.Linq": "4.0.0.0"
      },
      "dependencies": {
        "Google.GData.Client": "1.0.0-*",
        "Google.GData.Extensions": "1.0.0-*",
        "Google.GData.Spreadsheets": "1.0.0-*",
        "NAudio": "1.7.3",
        "Newtonsoft.Json": "1.0.0-*"
      }
    }
  }
}
